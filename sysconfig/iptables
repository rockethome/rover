*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:LOGACCEPT - [0:0]
:LOGDROP - [0:0]
:ROCKETHOME - [0:0]
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT 
-A INPUT -i lo -j ACCEPT 
-A INPUT -j ROCKETHOME 
-A INPUT -j LOGDROP 
-A LOGACCEPT -j LOG --log-prefix "NSEC: PacketAccept: " 
-A LOGACCEPT -j ACCEPT
-A LOGDROP -p tcp -m tcp --dport 80 -m comment --comment "Rule 10: Allow HTTP" -j ACCEPT 
-A LOGDROP -p tcp -m tcp --dport 443 -m comment --comment "Rule 11: Allow HTTPS" -j ACCEPT 
-A LOGDROP -p tcp -m tcp --dport 8443 -m comment --comment "Rule 11: Allow HTTPS/Alt" -j ACCEPT 
-A LOGDROP -p tcp -m tcp --dport 9876 -m comment --comment "Rule 20: Allow SSH" -j ACCEPT 
-A LOGDROP -p tcp -m tcp --dport 22 -m comment --comment "Rule 21: Allow SSH" -j LOGACCEPT 
-A LOGDROP -p tcp -m tcp --dport 25 -m comment --comment "Rule 22: Allow SMTP" -j LOGACCEPT 
-A LOGDROP -j LOG --log-prefix "NSEC: PacketDrop: " 
-A LOGDROP -j REJECT --reject-with icmp-host-prohibited 
